.tabs
	&--etapas
		margin: 20px 0
		background: none
		border: none
		.tabs__item
			text-align: center
			width: 190px
			&--active
				.tabs__title
					background: #9bb2fe
					color: #000
		.tabs__title
			background: #aaa
			color: #fff
			&:hover
				background: #aaa
		.tabs__fecha
			font-size: 12px